<h1>List of all the Wikis</h1>
<% @wikis.each do |wiki| %>

  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <p><%= link_to wiki.title, wiki %></p>
      </h4>
    </div>
  </div>
<% end %>

<div class="row">
  <div class="col-md-8">
    <% if policy(Wiki).new? %>
      <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
    <% end %>
  </div>
</div>
